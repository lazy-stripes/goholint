// {{.Opcode | printf "%02X"}}: LD A,(a16)		16 cycles
type {{name .}} struct {
	MultiStepsOp
}

func (op *{{name .}}) Tick() (done bool) {
	switch op.step {
	case 0:
		op.cpu.temp16 = uint16(op.cpu.NextByte())
	case 1:
		op.cpu.temp16 |= uint16(op.cpu.NextByte()) << 8
	case 2:
		op.cpu.A = op.cpu.MMU.Read(op.cpu.temp16)
		done = true
	}
	op.step++
	return
}


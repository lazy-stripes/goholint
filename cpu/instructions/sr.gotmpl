// CB {{.Opcode | printf "%02X"}}: {{.Instruction}}	{{.Register}}			8 cycles
type {{name .}} struct {
	SingleStepOp
}

func (op *{{name .}}) Execute(c *CPU) (done bool) {
	// Flags z 0 0 c
	c.F = 0x00
	if c.{{.Register}}&1 > 0 {
		c.F |= FlagC
	}
	c.{{.Register}} = (c.{{.Register}} >> 1){{if eq .Instruction "SRL"}} | (c.{{.Register}} & (1 << 7)){{end}}
	if c.{{.Register}} == 0 {
		c.F |= FlagZ
	}
	return true
}

